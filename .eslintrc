{
  "parser": "@babel/eslint-parser",
  "extends": ["plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "ignorePatterns": ["node_modules/"],
  "globals": {
    "$": true
  },
  "eslint.workingDirectories": [{ "mode": "auto" }]
}
